{
  "name": "vue-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "author": "Yang1206 <https://github.com/yang1206>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yang1206/vue-template.git"
  },
  "engines": {
    "node": "22"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "postinstall": "npx simple-git-hooks",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "test": "vitest",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.85.5",
    "@vueuse/core": "^13.7.0",
    "axios": "^1.11.0",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "qs": "^6.14.0",
    "vue": "^3.5.19",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@iconify-json/carbon": "^1.2.13",
    "@iconify-json/line-md": "^1.2.11",
    "@types/node": "^24.3.0",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.14.0",
    "@unocss/eslint-config": "^66.4.2",
    "@unocss/reset": "66.4.2",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/test-utils": "^2.4.6",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "eslint": "9.33.0",
    "eslint-plugin-format": "^1.0.1",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "simple-git-hooks": "^2.13.1",
    "typescript": "5.9.2",
    "unocss": "66.4.2",
    "unocss-preset-useful": "^1.0.1",
    "unplugin-auto-import": "20.0.0",
    "unplugin-vue-components": "^29.0.0",
    "unplugin-vue-macros": "^2.14.5",
    "unplugin-vue-router": "^0.15.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vitest": "^3.2.4",
    "vue-tsc": "3.0.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint:lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  }
}

{
  "name": "vue-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "author": "Yang1206 <https://github.com/yang1206>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yang1206/vue-template.git"
  },
  "engines": {
    "node": "22"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "postinstall": "npx simple-git-hooks",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "test": "vitest",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.62.0",
    "@vueuse/core": "^12.0.0",
    "axios": "^1.7.8",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.8",
    "qs": "^6.13.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@iconify-json/carbon": "^1.2.4",
    "@iconify-json/line-md": "^1.2.2",
    "@types/node": "^22.10.1",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.17",
    "@unocss/eslint-config": "^0.64.1",
    "@unocss/reset": "0.64.1",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/test-utils": "^2.4.6",
    "commitizen": "^4.3.1",
    "cz-git": "^1.11.0",
    "eslint": "9.16.0",
    "eslint-plugin-format": "^0.1.3",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "simple-git-hooks": "^2.11.1",
    "typescript": "5.6.3",
    "unocss": "0.64.1",
    "unocss-preset-useful": "^0.8.0",
    "unplugin-auto-import": "0.18.6",
    "unplugin-vue-components": "^0.27.5",
    "unplugin-vue-macros": "^2.13.4",
    "unplugin-vue-router": "^0.10.8",
    "vite": "6.0.1",
    "vite-plugin-vue-devtools": "^7.6.7",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vitest": "^2.1.6",
    "vue-tsc": "2.1.10"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint:lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  }
}
